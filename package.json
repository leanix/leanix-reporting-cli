{
  "name": "@leanix/reporting-cli",
  "version": "1.0.0-beta.21",
  "description": "Command line interface to develop custom reports for LeanIX EAM",
  "preferGlobal": true,
  "main": "./lib/app.js",
  "bin": {
    "lxr": "./bin/lxr"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "test": "jest",
    "test-install": "npm run build && npm -g install .",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm test"
  },
  "keywords": [
    "leanix",
    "pathfinder",
    "eam",
    "custom-report",
    "reporting",
    "reporting-cli"
  ],
  "author": "LeanIX GmbH <support@leanix.net>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/leanix/leanix-reporting-cli.git"
  },
  "devDependencies": {
    "@leanix/import-sort-style": "1.0.6",
    "@types/ejs": "3.1.5",
    "@types/inquirer": "7.3.3",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.7",
    "@types/mkdirp": "1.0.2",
    "@types/node": "14.18.63",
    "@types/request-promise-native": "1.0.21",
    "@types/rimraf": "3.0.2",
    "@types/tar": "6.1.13",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "husky": "4.3.8",
    "jest": "29.7.0",
    "prettier": "2.8.8",
    "prettier-plugin-import-sort": "0.0.7",
    "pretty-quick": "3.3.1",
    "ts-jest": "29.2.4",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "axios": "1.7.4",
    "chalk": "^4.1.0",
    "commander": "^12.0.0",
    "cross-spawn": "^5.1.0",
    "ejs": "^3.1.9",
    "form-data": "^4.0.0",
    "inquirer": "^7.3.3",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "opn": "^5.5.0",
    "rimraf": "^6.0.0",
    "tar": "^6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "importSort": {
    ".js, .ts": {
      "style": "@leanix/import-sort-style",
      "parser": "typescript"
    }
  }
}
