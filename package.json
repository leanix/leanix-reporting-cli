{
  "name": "@leanix/reporting-cli",
  "type": "commonjs",
  "version": "1.0.0-beta.28",
  "description": "Command line interface to develop custom reports for LeanIX EAM",
  "preferGlobal": true,
  "author": "LeanIX GmbH <support@leanix.net>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leanix/leanix-reporting-cli.git"
  },
  "keywords": [
    "leanix",
    "pathfinder",
    "eam",
    "custom-report",
    "reporting",
    "reporting-cli"
  ],
  "main": "./lib/app.js",
  "bin": {
    "lxr": "bin/lxr"
  },
  "scripts": {
    "build": "tsup src/app.ts -d lib --format cjs",
    "lint": "eslint",
    "test": "jest",
    "test-install": "npm run build && npm -g install .",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm test",
    "type-check": "npx tsc --noEmit"
  },
  "dependencies": {
    "axios": "1.13.5",
    "chalk": "4.1.2",
    "commander": "12.1.0",
    "cross-spawn": "7.0.6",
    "ejs": "3.1.10",
    "form-data": "4.0.5",
    "https-proxy-agent": "7.0.6",
    "inquirer": "8.2.7",
    "jwt-decode": "4.0.0",
    "lodash": "4.17.23",
    "mkdirp": "3.0.1",
    "opn": "5.5.0",
    "rimraf": "6.1.2",
    "tar": "7.5.7",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.19.0",
    "@types/ejs": "3.1.5",
    "@types/inquirer": "8.2.12",
    "@types/jest": "29.5.13",
    "@types/lodash": "4.17.23",
    "@types/mkdirp": "2.0.0",
    "@types/node": "22.19.8",
    "@types/request-promise-native": "1.0.21",
    "@types/rimraf": "3.0.2",
    "@types/tar": "6.1.13",
    "eslint": "9.32.0",
    "jest": "29.7.0",
    "ts-jest": "29.4.6",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  }
}
